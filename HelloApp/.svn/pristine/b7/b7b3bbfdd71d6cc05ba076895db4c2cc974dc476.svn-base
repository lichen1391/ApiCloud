<!doctype html>	
<html>	
  <head>	
    <meta charset="UTF-8">	
    <meta name="viewport" content="maximum-scale=1.0,minimum-scale=1.0,user-scalable=0,width=device-width,initial-scale=1.0"/>	
    <meta name="format-detection" content="telephone=no,email=no,date=no,address=no">	
    <title>window</title>	
    <link rel="stylesheet" type="text/css" href="../css/api.css" />
 	<link rel="stylesheet" type="text/css" href="../css/icon/iconfont.css" />
 	<link rel="stylesheet" type="text/css" href="../css/head_foot.css" />
    	
    <style>	
    </style>	
  </head>	
  <body>	
    <div id="wrap">	
      <!-- 第一头部 -->	
      <div id="firstHeader" class="titlebar activebar">	
        <div class="topbar">	
          <div class="citylist fl" id="citylist" tapmode onclick="bmap()">北京<img src="../image/title_home_arrow_down_normal.png" alt="" class="citylistarrow"></div>	
          <div class="search firstSearch" tapmode onclick="">	
              <img src="../image/ic_home_search.png" alt="">输入商户名、地点	
          </div>	
          <img src="../image/send_msg.png" alt="" class="fr headerico hr01"  tapmode="" onclick="">	
        </div>	
      </div>	
      <div id="main">	
        	
      </div>	
      <div id="footer">	
        <ul>	
          <li class="bottom_li" tapmode="activebtn0 activebtn" onclick="">
          	<i class="iconfont icon-homefill"></i>	
            <div class="bottom_btn bbtn01 weixin activebtn activebtn0">首页</div>	
          </li>	
          <li class="bottom_li" tapmode="activebtn1 activebtn" onclick="frieds_window()">
          	<i class="iconfont icon-discover"></i>	
            <a class="bottom_btn bbtn02 communicate">校友</a>	
          </li>	
          <li class="bottom_li" tapmode="activebtn2 activebtn" onclick="activity_window()">
          	<i class="iconfont icon-circle"></i>	
            <a class="bottom_btn bbtn03 discover">活动</a>	
          </li>	
          <li class="bottom_li" tapmode="activebtn3 activebtn" onclick="self_window()">
          	<i class="iconfont icon-profile"></i>
            <a class="bottom_btn bbtn04 home">我的</a>	
          </li>	
        </ul>	
      </div>	
    </div>	
  </body>	
  <script type="text/javascript" src="../script/api.js"></script>
  <script type="text/javascript" src="../script/footer.js"></script>
